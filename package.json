{
  "name": "deno-npm-demo",
  "version": "0.1.0",
  "description": "A demo using NPM packages with the Deno runtime",
  "main": "index.js",
  "type": "module",
  "author": "David Bailey <davidbailey00@outlook.com>",
  "license": "ISC",
  "scripts": {
    "prepare": "snowpack --include demos/**/*.js",
    "preact": "deno --importmap=web_modules/import-map.json demos/preact.js",
    "rxjs": "deno --importmap=web_modules/import-map.json demos/rxjs.js",
    "immer": "deno --importmap=web_modules/import-map.json demos/immer.js"
  },
  "dependencies": {
    "rollup": "^2.0.6"
  }
}
